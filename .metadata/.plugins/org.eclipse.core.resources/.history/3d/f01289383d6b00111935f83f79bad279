import android.content.Context;
import android.widget.TextView;

public class ChildTextView extends TextView {
	private Child myChild;

	public ChildTextView(Context context, Child aChild) {
		super(context);
		myChild = aChild;
		setText(myChild.getName());
		setOnClickListener();
	}
	
